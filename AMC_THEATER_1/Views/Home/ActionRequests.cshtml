
@model IEnumerable<AMC_THEATER_1.Models.TheaterViewModel>
@{

    ViewBag.CurrentAction = "ActionRequests";
    ViewBag.ShowBasicNavbar = false;
    ViewBag.ShowNavbar = true;
    ViewBag.ShowSideBar1 = true;
    ViewBag.Login = false;
    ViewBag.ShowHomeIcon = true;
    ViewBag.Module = true;
}


@if (ViewBag.Message != null)
{
    <div class="alert alert-warning">@ViewBag.Message</div>
}
<div class="fw-bold">
    <h1 class="text-center bg-light p-3 pe-5 w-100">Action Requests</h1>
</div>

<!-- Use the Common Table View -->
@Html.Partial("CommonTableView", Model)
